{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api",
            "dest": "api/main.py"
        },
        {
            "src": "/api/(.*)",
            "dest": "api/main.py"
        },
        {
            "src": "/api",
            "dest": "api/index.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        }
    ]
}
